{
  "version": 1,
  "name": "SshAccess",
  "statements": [
    { "effect": "Allow",
      "actions": [ "tcptunnel:Open" ],
      "conditions":{
        "And": [
          {"NumberEqual": [  { "Attribute": "tcptunnel:port" }, 42] },
          { "StringEqual": [ { "Attribute": "tcptunnel:host" }, "localhost"] }
        ]
      }
    }
  ]
}
